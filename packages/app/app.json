{
  "expo": {
    "name": "Pulse",
    "slug": "Pulse",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pulse",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f1f1f"
    },
    "extra": {
      "DSN": "https://04ce65a60dd9b50514b823edaa61c830@o4504234803068928.ingest.us.sentry.io/4507992564498432"
    },
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gempoint.pulse"
    },
    "android": {
      "package": "com.gempoint.pulse",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1f1f1f"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@wwdrew/expo-spotify-sdk",
        {
          "clientID": "f61910ffe6754c74825ec6dd21d7bcb0",
          "scheme": "com.gempoint.pulse",
          "host": "auth"
        }
      ],
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "gempoint",
          "project": "pulse",
          // If you are using a self-hosted instance, update the value of the url property
          // to point towards your self-hosted instance. For example, https://self-hosted.example.com/.
          "url": "https://sentry.io/"
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
